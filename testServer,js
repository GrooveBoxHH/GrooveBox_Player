var Server = require('./libs/server');

var myServer = new Server(1337);

myServer.init(function postCallback(url) {

	console.log('callback called' + url);
}, 
function nextCallback() {

	console.log('nextCallback called');
},
function stopCallback() {
	
	console.log('stopCallback called');
});

myServer.start();

